class MyInt{
    int inner; 
    def MyInt(int a){
        this.inner = a;
    }
}
def print(MyInt i){
    printf("%d\n",i.inner);
}
MyInt i1 = MyInt(3);
MyInt i2 = i1;
print(i1); // prints 3
print(i2); // prints 3
i2.inner = 4;
print(i1); // prints 3
print(i2); // prints 4

