def cat(lang.c.string s){
        File f = File(s,'r');
	while(true){
	        auto t = f.read();
		if(t==-1) break;
                print(chr(t));
        }
        f.close();
}

/* ONLY SHOWS FILES */
def ls(lang.c.string s='.'){
	mapdir(s,println);
}

def bash(){
	system('bash');
}

def python(){
	system('python');
}
