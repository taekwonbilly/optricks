def FOR(void% i, bool% b=true, void% c){
	i();
	while(b()){
		c();
	}

}
def repeat(void% a, int b){
	for(int i=0; i<b; i++) a();
}

def repeat10(void% a){
	for(int i=0; i<10; i++) a();
}
