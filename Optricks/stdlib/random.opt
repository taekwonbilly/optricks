def FOREACH( int[] a, int& b, void% v){
	for(int i=0; i<a.length;i++){
		b =a[i]
		v()
	}

}
class primitive random{
	static def int rand_interval(int min, int max){
		assert(max>min);
		auto range = max-min;
		auto buckets = RAND_MAX/range;
		auto limit = buckets*range;
		int r;
		do{
			r = rand();
		}while(r>=limit);
		return min+ r/buckets;
	}

}
