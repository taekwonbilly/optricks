def FOREACH( int[] a, int& b, void% v){
	for(int i=0; i<a.length;i++){
		b =a[i]
		v()
	}

}
class primitive random{
//	static int RAND_MAX=4;
	static def int rand_interval(int min, int max){
		assert(max>min);
//		const range = max-min;
//		const buckets = RAND_MAX/range;
//		const limit = buckets*range;
		auto range = max-min;
		auto buckets = 1000/range;
		auto limit = buckets*range;
		int r;
		do{
			r = rand();
		}while(r>=limit);
		return min+ r/buckets;
	}

}
def FOR(void% i, bool% b=true, void% c){
	i();
	while(b()){
		c();
	}

}
def repeat(void% a, int b){
	for(int i=0; i<b; i++) a();
}

def repeat10(void% a){
	for(int i=0; i<10; i++) a();
}


def inc(int& a){
	a++;
}
def void lazy(int% i){
       println("before: lazy");
       printf("%d\n", i());
       println("after: lazy");
}
